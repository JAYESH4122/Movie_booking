<!DOCTYPE html>
<html lang="en">
<head>
  <title>Admin Dashboard</title>
</head>
<body>
  <h1>Admin Dashboard</h1>
  <table border="1">
    <thead>
      <tr>
        <th>Movie</th>
        <th>Seats</th>
        <th>User</th>
        <th>Booked Time</th>
      </tr>
    </thead>
    <tbody>
      <% reservations.forEach(reservation => { %>
        <tr>
          <td><%= reservation.movieId.name %></td>
          <td><%= reservation.seats.join(', ') %></td>
          <td><%= reservation.user %></td>
          <td><%= new Date(reservation.createdAt).toLocaleString('en-IN', { 
            day: 'numeric', 
            month: 'long', 
            year: 'numeric', 
            hour: '2-digit', 
            minute: '2-digit' 
          }) %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
